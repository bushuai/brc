{
  "name": "@bushuai/brc",
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "packageManager": "pnpm@7.27.1",
  "version": "0.0.5",
  "bin": {
    "brc": "bin/brc.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "dev": "jiti index.ts",
    "build": "tsup index.ts",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@clack/core": "^0.3.2",
    "@clack/prompts": "^0.6.3"
  },
  "devDependencies": {
    "bumpp": "^9.0.0",
    "bun-types": "^0.5.0",
    "jiti": "^1.17.2",
    "tsup": "^6.6.3"
  },
  "engines": {
    "node": ">=14.19.0"
  }
}
